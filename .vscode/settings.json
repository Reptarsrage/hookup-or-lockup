{
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.format": true
  },
  "cSpell.words": [],
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/node_modules": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/build": true,
    "**/package-lock.json": true
  },
  "workbench.colorCustomizations": {
    "activityBar.activeBackground": "#5ad978",
    "activityBar.background": "#5ad978",
    "activityBar.foreground": "#15202b",
    "activityBar.inactiveForeground": "#15202b99",
    "activityBarBadge.background": "#8064db",
    "activityBarBadge.foreground": "#e7e7e7",
    "commandCenter.border": "#15202b99",
    "sash.hoverBorder": "#5ad978",
    "statusBar.background": "#31cf56",
    "statusBar.foreground": "#15202b",
    "statusBarItem.hoverBackground": "#27a645",
    "statusBarItem.remoteBackground": "#31cf56",
    "statusBarItem.remoteForeground": "#15202b",
    "titleBar.activeBackground": "#31cf56",
    "titleBar.activeForeground": "#15202b",
    "titleBar.inactiveBackground": "#31cf5699",
    "titleBar.inactiveForeground": "#15202b99"
},
  "peacock.color": "#31cf56"
}
