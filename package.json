{
  "name": "hookup-or-lockup",
  "productName": "Hookup or Lockup",
  "private": true,
  "sideEffects": false,
  "version": "0.0.2",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/reptarsrage/hookup-or-lockup.git"
  },
  "author": {
    "name": "Reptarsrage",
    "email": "reptarsrage@gmail.com",
    "url": "https://github.com/reptarsrage"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/reptarsrage/hookup-or-lockup/issues"
  },
  "keywords": [
    "vite",
    "react",
    "remix",
    "typescript"
  ],
  "homepage": "https://github.com/reptarsrage/hookup-or-lockup#readme",
  "scripts": {
    "icons": "npx @svgr/cli --out-dir app/components/icons -- app/icons",
    "icons:watch": "npm-watch icons",
    "dev:svg": "run-s icons icons:watch",
    "build": "run-s build:*",
    "build:css": "npm run generate:css -- --minify",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:css": "npm run generate:css -- --watch",
    "dev:remix": "cross-env NODE_ENV=development binode -- @remix-run/dev:remix dev",
    "format": "prettier --write .",
    "generate:css": "tailwindcss -o ./app/styles/tailwind.css",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "prepare": "husky install",
    "release": "standard-version",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "remix-serve build",
    "start:mocks": "cross-env SESSION_SECRET=secret binode -- @remix-run/serve:remix-serve build",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 \"npx cypress open\"",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811 \"npx cypress run\"",
    "typecheck": "tsc -b && tsc -b cypress",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@prisma/client": "^4.5.0",
    "@react-hook/size": "^2.1.2",
    "@react-hook/window-size": "^3.1.1",
    "@react-spring/web": "^9.6.1",
    "@remix-run/node": "^1.9.0",
    "@remix-run/react": "^1.9.0",
    "@remix-run/serve": "^1.9.0",
    "@remix-run/server-runtime": "^1.9.0",
    "@use-gesture/react": "^10.2.23",
    "axios": "^1.2.2",
    "clsx": "^1.2.1",
    "html-to-text": "^9.0.3",
    "isbot": "^3.6.3",
    "lru-cache": "^10.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.2.0",
    "react-confetti": "^6.1.0",
    "react-dom": "^18.2.0",
    "react-lottie": "^1.2.3",
    "superjson": "^1.12.3",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@remix-run/dev": "^1.9.0",
    "@remix-run/eslint-config": "^1.9.0",
    "@svgr/cli": "^8.0.0",
    "@svgr/plugin-jsx": "^8.0.0",
    "@svgr/plugin-prettier": "^8.0.0",
    "@svgr/plugin-svgo": "^8.0.0",
    "@testing-library/cypress": "^9.0.0",
    "@testing-library/dom": "^9.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/eslint": "^8.21.0",
    "@types/html-to-text": "^9.0.0",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/react-lottie": "^1.2.6",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-c8": "^0.31.0",
    "autoprefixer": "^10.4.13",
    "binode": "^1.0.5",
    "c8": "^8.0.0",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cypress": "^12.5.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.26.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "happy-dom": "^10.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.1.0",
    "npm-run-all": "^4.1.5",
    "npm-watch": "^0.11.0",
    "postcss": "^8.4.18",
    "prettier": "3.0.0",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "prisma": "^4.8.0",
    "standard-version": "^9.5.0",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.2.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^5.0.0",
    "vite": "^4.1.1",
    "vite-tsconfig-paths": "^4.0.0",
    "vitest": "^0.31.0"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "watch": {
    "icons": {
      "patterns": [
        "icons"
      ],
      "extensions": "svg",
      "quiet": false
    }
  }
}
